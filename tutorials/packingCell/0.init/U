FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "2050";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "solid.*"
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
    ".*.cyc"
    {
        type            cyclic;
    }
}


